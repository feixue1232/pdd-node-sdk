{
  "name": "pdd-node-sdk",
  "version": "0.0.1",
  "description": "pdd node sdk",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write \"+(src|scripts)/**/*.ts\"",
    "test": "mocha --require ts-node/register test/*.ts test/**/*.ts",
    "generate": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node -r tsconfig-paths/register ./scripts/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lywzx/pdd-node-sdk.git"
  },
  "keywords": [
    "pdd",
    "pinduoduo",
    "pinduoke"
  ],
  "author": "liuyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lywzx/pdd-node-sdk/issues"
  },
  "homepage": "https://github.com/lywzx/pdd-node-sdk#readme",
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/chai": "^4.2.9",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "tiny-pinyin": "^1.3.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "async": "^3.1.1",
    "axios": "^0.19.2",
    "full-icu": "^1.3.0",
    "lodash": "^4.17.15"
  }
}
